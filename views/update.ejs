<html>
    <head>
        <script src="https://code.jquery.com/jquery-3.4.1.js" integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=" crossorigin="anonymous"></script>
        <script src="/assets/jquery-ui.js" type="text/javascript"></script>
        <script src="/assets/todo-list.js" type="text/javascript"></script>
        <link href="/assets/styles.css" rel="stylesheet" type="text/css">
    </head>
    <body>
        <form action="/update" method="post" id="update">
        <h1>Update Registration Details</h1>
        <div id="todo-table">
            <table>
                <tr>
                    <td width="20%">Name :</td>
                    <td width="80%"><input type="text" size="20" name="name" required="required" value="<%= data.name %>"/></td>
                </tr>
                <tr>
                    <td>Age :</td>
                    <td><input type="number" size="20" name="age" required="required" value="<%= data.age %>"/></td>
                </tr>
                <tr>
                    <td>City :</td>
                    <td><input type="text" size="20" name="city" required="required" value="<%= data.city %>"/></td>
                </tr>
                <tr>
                    <td>State :</td>
                    <td><input type="text" size="20" name="state" required="required" value="<%= data.state %>"/></td>
                </tr>
                <tr>
                    <td>Zip :</td>
                    <td><input type="number" size="20" name="zip" required="required" value="<%= data.zip %>"/></td>
                </tr>
                <tr>
                    <td><input type="hidden" name="seq" value="<%= data.seq %>"/></td>
                    <td><button type="submit">Update</button>&nbsp;&nbsp;<button type="button" onClick="js:resetForm();">Reset</button></td>
                </tr>
            </table>
        </div>
    </form>
    </body>
</html>

<script type="text/javascript">
    function resetForm()
    {
        window.location.href = window.location.href;
    }
</script>